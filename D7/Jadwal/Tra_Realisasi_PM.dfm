    SQLInsert.Strings = (
      'INSERT INTO NOTA_JUAL_M'
      
        '  (ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, TGL_' +
        'TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_EDIT,' +
        ' OPR_TERMINATED, TGL_TERMINATED, REASON_TERMINATED, STATUS, OLD_' +
        'STATUS, NAMA_TTD2, NAMA_TTD3, NAMA_TTD4, TGL_TTD2, TGL_TTD3, TGL' +
        '_TTD4, JENIS_NOTA, TGL_RENCANA_PENGIRIMAN, KD_OUTLET, NO_SP, TGL' +
        '_SP, TGL_EXPIRED, KODE_SALES, TOTAL_GROSS, DISC_LVL_1, DISC_LVL_' +
        '2, SUB_TOTAL_1, NETTO, DEPO, NO_TRANSAKSI_LAMA, jenis_bayar, tot' +
